{% if project['dynamicStoreMode'] %}
        fastcgi_param SPRYKER_CURRENT_REGION "{{ endpointData['identifier'] }}";
{% else %}
        fastcgi_param APPLICATION_STORE "{{ endpointData['identifier'] }}";
{% endif %}
{% if endpointData['code-bucket'] %}
        fastcgi_param SPRYKER_CODE_BUCKET "{{ endpointData['code-bucket'] }}";
{% endif %}
        fastcgi_param SPRYKER_SEARCH_NAMESPACE "{{ storeServices['search']['namespace'] | default(endpointData['services']['search']['namespace'])  }}";
        fastcgi_param SPRYKER_KEY_VALUE_STORE_NAMESPACE "{{ storeServices['key_value_store']['namespace'] | default(endpointData['services']['key_value_store']['namespace'])  }}";
        fastcgi_param SPRYKER_BROKER_NAMESPACE "{{ storeServices['broker']['namespace'] | default(endpointData['services']['broker']['namespace'])  }}";
